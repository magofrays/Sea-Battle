diagrams:
    lab4_diagram:
      type: class
      glob:
        - game/src/main.cpp
      include:
        paths:
          - game/src/IO/gameController.h
          - game/src/IO/gameTracker.h
          - game/src/SeaBattle.h
          - game/src/states/Game.h
          - game/src/IO/GUI/*.h
          - game/src/IO/TUI/*.h
          - game/src/RW/controlReader.h
          - game/src/states/*.h

      exclude:
        elements:
          - fontSize
        namespaces:
          - std
compilation_database_dir: build
